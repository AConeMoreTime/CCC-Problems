#include <bits/stdc++.h>
using namespace std;
struct ST {
    string s;
    int num;
};


int main(int argc, const char * argv[]) {
    
    int n; cin >> n;
    ST s1[100];
    for(int i = 0; i < n; i++){
        cin >> s1[i].num;
        cin >> s1[i].s;
    }
    for(int i = 0; i < n; i++){
        for(int j = 0; j < s1[i].num; j++){
            cout << s1[i].s;
        }
        cout << endl;
    }
    return 0;
}
